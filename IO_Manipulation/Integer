#include<iostream>
using namespace std;
int main()
{
    int a=26,b=20;
    cout<<a<<" "<<b<<" "<<"\n";
    cout<<hex;
    cout<<a<<" "<<b<<" "<<"\n";
    cout<<oct;
    cout<<a<<" "<<b<<" "<<"\n";
    cout<<dec;
    cout<<a<<" "<<b<<" "<<"\n";
    cout<<showbase;
    cout<<oct;
    cout<<a<<"\n";
    cout<<hex;
    cout<<a<<"\n";
    cout<<showpos;
    cout<<a<<"\n";
    cout<<uppercase;
    cout<<a<<"\n";
    return 0;


}
