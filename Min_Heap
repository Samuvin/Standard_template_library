#include <iostream>
#include <bits/stdc++.h>
using namespace std;
int main()
{
    int size, a;
    cin >> size;
    priority_queue<int, vector<int>, greater<int>> pq;
    for (int i = 0; i < size; i++)
    {
        cin >> a;
        pq.push(a);
    };
    cout << pq.size() << " "
         << "\n";
    cout << pq.top() << " "
         << "\n";
    while (!pq.empty())
    {
        cout << pq.top() << " ";
        pq.pop();
    }
    return 0;
    // using array as heap(priority-queue<int> pq(arr,arr+n));
}
