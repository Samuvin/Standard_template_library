#include<iostream>
#include<bits/stdc++.h>
using namespace std;
int main()
{
    vector<int>v={1,4,6,2,9,45,97};
    sort(v.begin(),v.end());

    for(auto& it: v)
    {
        cout<<it<<" ";
    }

    sort(v.begin(),v.end(),greater<int>());

    for(auto& it: v)
    {
        cout<<it<<" ";
    }

    int arr[3]={2,5,1};
    sort(arr,arr+3);
    for(int i=0;i<3;i++)
    {
        cout<<arr[i]<<" ";
    }

    reverse(v.begin(),v.end());
    return 0;
}
