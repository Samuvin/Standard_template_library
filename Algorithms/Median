#include <iostream>
#include <bits/stdc++.h>
using namespace std;
int main()
{
    int row;
    int col;
    cin >> row >> col;
    int arr[row][col];
    for (int i = 0; i < row; i++)
    {
        for (int j = 0; j < col; j++)
        {
            cin >> arr[i][j];
        }
    }
    int min = arr[0][0];
    int max = arr[0][-1];
    for (int i = 1; i < row; i++)
    {
        if (arr[i][0] < min)
            min = arr[i][0];
        if (arr[i][-1] > max)
            max = arr[i][-1];
    }
    int mid=(row*col+1)/2;
    while(min<max)
    {
        int mid=(min+mid)/2;
        int midpos=0;
        

    }
}
