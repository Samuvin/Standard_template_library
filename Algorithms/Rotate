#include <iostream>
#include <bits/stdc++.h>
using namespace std;
int main()
{
    vector<int> v = {10, 20, 30, 40, 50, 60, 70, 80};
    rotate(v.begin(), v.begin() + 2, v.end());
    for (int x : v)
        cout << x << " ";
    cout << "\n";

    // arr
    int arr[] = {10, 20, 30, 40, 50, 60, 70, 80};
    rotate(arr, arr + 2, arr + 6);
    for (int x : arr)
        cout << x << " ";
}
