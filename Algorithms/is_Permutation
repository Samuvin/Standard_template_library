#include <iostream>
#include <bits/stdc++.h>
using namespace std;
int main()
{
    vector<int> v1 = {10, 20, 30, 5};
    vector<int> v2 = {20, 10, 5, 30};
    if (is_permutation(v1.begin(), v1.end(), v2.begin()))
        cout << "YES"
             << "\n";
    else
    {
        cout << "NO"
             << "\n";
    }
    int arr1[] = {30, 20, 10};
    int arr2[] = {20, 10, 30};
    if (is_permutation(arr1, arr1 + 3, arr2))
        cout << "YES"
             << "\n";
    else
        cout << "NO"
             << "\n";
}
